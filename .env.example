# TriageAssist - Single Environment Configuration
# Copy this file to .env and fill in your credentials

# ============================================
# Supabase Configuration (Used by all services)
# ============================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# ============================================
# Backend API Configuration
# ============================================
BACKEND_PORT=3000
BACKEND_NODE_ENV=development
BACKEND_CORS_ORIGIN=http://localhost:5173
STORAGE_BUCKET=patient-audio-raw

# ============================================
# Frontend Configuration
# Note: VITE_ prefix is required for Vite to expose these to the frontend
# ============================================
VITE_API_URL=http://localhost:3000
# Set these to the same values as SUPABASE_URL and SUPABASE_ANON_KEY above
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
VITE_DEEPGRAM_API_KEY=your-deepgram-api-key

# ============================================
# n8n-mock Configuration
# ============================================
N8N_MOCK_PORT=3001
N8N_MOCK_NODE_ENV=development
N8N_WEBHOOK_URL=http://localhost:3001/webhook/process-visit
PROCESSING_DELAY_MIN=10000
PROCESSING_DELAY_MAX=15000
USE_REAL_DEEPGRAM=false

# ============================================
# OpenAI Configuration (n8n-mock)
# ============================================
OPENAI_API_KEY=your-openai-api-key

# ============================================
# Deepgram Configuration (n8n-mock - optional)
# ============================================
DEEPGRAM_API_KEY=your-deepgram-api-key
